@model List<PayInc_Customer_web.Areas.PaymentManagement.Models.BankListResponse>


@if (Model != null)
{
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-3">
                <a href="javascript:;" class="symbol symbol-lg-150 showbank" data-item="@item.bankId">
                    <span class="symbol-label">
                        <img src="@(item.bankLogo.Replace("png","jpeg"))" class="img-fluid" />
                    </span>
                    <span class="clearfix text-center">@item.accountNumber - (@item.bankName)</span>
                </a>
            </div>
        }
    </div>

    <div class="clearfix"></div>
    <div class="d-flex justify-content-between border-top pt-10 mt-15">
        <div class="mr-2">
            <button type="button" class="btn btn-light-primary font-weight-bolder px-9 py-4 prev-step" data-wizard-type="action-prev">Previous</button>
        </div>
    </div>
    <script src="~/js/paymentrequest_wizard.js"></script>
}
