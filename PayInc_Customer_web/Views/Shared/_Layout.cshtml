<script>
  $(document).ready(function () {
    $.ajax({
      type: "GET",
      url: "/wallet/GetBalance",
      dataType: "json",
      success: function (data) {
        //debugger;
          if (data!="") {
              $.each(data, function (key, value) {
                  if (value.walletTypeId === 1) {
                      $(".wallet_balance").text(value.customerEffectiveBalance);
                      $("#wallet_balance").text(value.customerEffectiveBalance);
                      $("#wallet_mainbalance").text(value.customerMainAccountBalance);
                      $("#wallet_reservedbalance").text(value.customerSystemReservedBalance);
                      $("#wallet_unclearbalance").text(value.customerUnclearBalance);
                      $("#wallet_lienbalance").text(value.customerLienBalance);
                  }
                  else if (value.walletTypeId === 2) {
                      $("#aeps_balance").text(value.customerEffectiveBalance);
                      $("#aeps_mainbalance").text(value.customerMainAccountBalance);
                      $("#aeps_reservedbalance").text(value.customerSystemReservedBalance);
                      $("#aeps_unclearbalance").text(value.customerUnclearBalance);
                      $("#aeps_lienbalance").text(value.customerLienBalance);
                      $(".aeps_balance").text(value.customerEffectiveBalance);
                  }
              });
          }
        

      },
    });
  });
</script>
